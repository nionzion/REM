    <FontFamily x:Key="LatoBold">pack://application;,,,/Fonts/#Lato Bold</FontFamily>
    <FontFamily x:Key="FontAwesome">pack://application;,,,/Fonts/#FontAwesome</FontFamily>
    <FontFamily x:Key="FontAwesome5Light">pack://application;,,,/Fonts/#Font Awesome 5 Pro Light</FontFamily>
    <FontFamily x:Key="PlayFairDisplayRegular">pack://application;,,,/Fonts/#Playfair Display Regular</FontFamily>

    <Style TargetType="{x:Type Control}" x:Key="BaseStyle">
        <Setter Property="FontFamily" Value="{StaticResource LatoThin}" />
