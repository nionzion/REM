    <FontFamily x:Key="LatoRegular">pack://application;,,,/Fonts/#Lato Regular</FontFamily>-->
    <FontFamily x:Key="LatoThin">pack://application;,,,/Fonts/#Nunito Sans Light</FontFamily>
    <FontFamily x:Key="LatoRegular">pack://application;,,,/Fonts/#Nunito Sans Regular</FontFamily>
    <FontFamily x:Key="NunitoBold">pack://application;,,,/Fonts/#Nunito Sans Bold</FontFamily>
